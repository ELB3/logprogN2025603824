    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Sexo" type="String" array="False" size=""/>
            <declare name="I" type="String" array="False" size=""/>
            <output expression="&quot;introducir sexo M o H&quot;" newline="True"/>
            <input variable="Sexo"/>
            <output expression="&quot;introducir Inicial&quot;" newline="True"/>
            <input variable="I"/>
            <if expression="(Sexo==&quot;M&quot;)AND(I==&quot;A&quot; OR I==&quot;B&quot; OR I==&quot;C&quot; OR I==&quot;D&quot; OR I==&quot;E&quot; OR I==&quot;F&quot; OR I==&quot;G&quot; OR I==&quot;H&quot; OR I==&quot;I&quot; OR I==&quot;J&quot; OR I==&quot;K&quot; OR I==&quot;L&quot; OR I==&quot;M&quot;)">
                <then>
                    <output expression="&quot;Estas en el grupo A&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="(Sexo==&quot;M&quot;)AND(I == &quot;N&quot; OR I == &quot;O&quot; OR I == &quot;P&quot; OR I == &quot;Q&quot; OR I == &quot;R&quot; OR I == &quot;S&quot; OR I == &quot;T&quot; OR I == &quot;U&quot; OR I == &quot;V&quot; OR I == &quot;W&quot; OR I == &quot;X&quot; OR I == &quot;Y&quot; OR I == &quot;Z&quot;)">
