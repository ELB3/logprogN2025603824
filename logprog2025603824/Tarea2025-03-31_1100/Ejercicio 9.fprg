                                    <output expression="&quot;Estas en el grupo B&quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot;Estas en el grupo A&quot;" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
